                  <property name="styleSheet">
                   <string notr="true">/* Đặt objectName cho QLineEdit là &quot;searchBar&quot; */
QLineEdit {
    /* nền nhẹ nhàng */
    background-color: white;
    /* bo tròn full height (nếu height=32px thì radius=16px) */
    border-radius: 16px;
	padding: 3px 6px 3px 6px;
}

/* Placeholder màu xám nhạt */
QLineEdit#searchBar::placeholder {
    color: #A0A0A0;
}

/* Khi focus thì nền trắng sáng hơn, có viền nhẹ */
QLineEdit#searchBar:focus {
    background-color: #FFFFFF;
    border: 1px solid #D0D0D0;
}

/* Nếu cần disabled state */
QLineEdit#searchBar:disabled {
    background-color: #ECECEC;
    color: #A0A0A0;
}
